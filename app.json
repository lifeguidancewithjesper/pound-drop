{
  "expo": {
    "name": "Pound Drop",
    "owner": "lifeguidancewithjesper",
    "slug": "pounddrop-2024",
    "version": "1.0.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "description": "Your personal weight loss tracking companion with meal logging, habit tracking, and progress insights.",
    "keywords": [
      "weight loss",
      "health",
      "fitness",
      "meal tracking",
      "diet",
      "wellness"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lifeguidancewithjesper.pounddrop",
      "buildNumber": "19",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "NSCameraUsageDescription": "Take photos of your meals for tracking",
        "NSPhotoLibraryUsageDescription": "Select photos from your library for meal logging",
        "NSUserNotificationUsageDescription": "Send reminders for meal logging and habit tracking",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.lifeguidancewithjesper.pounddrop",
      "versionCode": 2,
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f952f40e-baa9-4bfa-bd48-8be72c614fd7"
      }
    },
    "plugins": []
  }
}
